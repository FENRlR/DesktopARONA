<Window x:Class="ARONA_dst.Windows.info"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ARONA_dst.Windows"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Icon="{x:Null}"
        Width="410" Height="220" WindowStyle="ToolWindow">
    <Grid>
        <TextBlock Name="titlebox" Width="400" Height="200" Background="Transparent"
                   FontFamily="GyeonggiTitle Medium" FontSize="24" FontWeight="ExtraBold"
                   TextAlignment="Center" Padding="0,70,0,0" Foreground="Black"
                   Text="Desktop ARONA Ver. 2.0a"
                   />
        <TextBlock Name="sigbox" Width="400" Height="200" Background="Transparent"
           FontFamily="GyeonggiTitle Medium" FontSize="20" FontWeight="Normal"
           TextAlignment="Center" Padding="0,95,0,0" Foreground="#29415a"
           Text="Developed by FENRIR"
           />
    </Grid>
</Window>
